package al;

import java.util.Scanner;

public class Fibonacci {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		int[] ar= new int[n];
		
		fib(n, ar);
		
		sc.close();
	}
	
	
	
	public static int fib(int n, int[] ar) {
		int r=Integer.MIN_VALUE;
		
		if(n<=1)
			return 1;
		else {
			
			r=fib(n-1, ar)+fib(n-2, ar);
			ar
		}
		
		
		return r;
	}
}


